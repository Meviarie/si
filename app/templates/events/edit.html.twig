{% extends 'base.html.twig' %}

{% block title %}{{ 'title.events_edit'|trans({'%id%': events.id}) }}{% endblock %}

{% block body %}
    <h1>{{ 'title.events_edit'|trans({'%id%': bookmark.id}) }}</h1>
    {{ form_start(form, { method: 'post', action: url('events_edit', {'id': events.id}) }) }}

    {% form_theme form with ['bootstrap_3_horizontal_layout.html.twig', 'Form/form_errors.html.twig'] %}

    {{ form_widget(form) }}
    <input type="submit" value="{{ 'action.save_events'|trans }}" class="btn btn-success" />
    {{ form_end(form) }}
{% endblock %}